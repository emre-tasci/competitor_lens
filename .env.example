# Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://postgres.xxxxx:YOUR_PASSWORD@aws-1-region.pooler.supabase.com:6543/postgres?pgbouncer=true
DIRECT_URL=postgresql://postgres.xxxxx:YOUR_PASSWORD@aws-1-region.pooler.supabase.com:5432/postgres

# xAI API
XAI_API_KEY=xai-your-api-key-here

# AWS S3
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
